    "start": "NODE_ENV=production node dist/index.js",
    "build": "tsc"
  },
  "dependencies": {
    "@types/node": "^11.13.4",
    "moment": "^2.24.0",
    "typescript": "^3.4.3"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/mock-fs": "^3.6.30",
    "jest": "^24.7.1",
    "mock-fs": "^4.8.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.0.3",
    "tslint": "^5.15.0"
  }
}
