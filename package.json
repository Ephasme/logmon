  "license": "MIT",
  "scripts": {
    "lint": "tslint -p . --fix",
    "start:logs-debug": "nodemon --watch src --inspect=5859 -r ts-node/register ./src/__tests_utils__/index.ts --filename data/access.log",
    "start:app-debug": "nodemon --watch src --inspect=5858 -r ts-node/register ./src/index.ts --filename data/access.log",
    "start:all-debug": "concurrently \"yarn run start:write-logs\" \"yarn run start:app-debug\""
  },
  "dependencies": {
    "@types/node": "^11.13.4",
    "immutable": "^4.0.0-rc.12",
    "moment": "^2.24.0",
    "typescript": "^3.4.3",
    "yarg": "^1.0.8"
  },
  "devDependencies": {
    "@types/faker": "^4.1.5",
    "@types/jest": "^24.0.11",
    "@types/mock-fs": "^3.6.30",
    "concurrently": "^4.1.0",
    "faker": "^4.1.0",
    "jest": "^24.7.1",
    "mock-fs": "^4.8.0",
    "nodemon": "^1.18.11",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.0.3",
    "tslint": "^5.15.0"
  }
}
