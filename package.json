  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/mock-fs": "^3.6.30",
    "jest": "^24.7.1",
    "mock-fs": "^4.8.0",
    "nodemon": "^1.18.11",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.0.3",
    "tslint": "^5.15.0"
  }
