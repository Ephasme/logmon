{
  "name": "logmon-ts",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "clean": "./clean.sh",
    "lint:fix": "tslint --project tsconfig.json --fix",
    "test:cover": "jest --coverage",
    "test:watch": "jest --watch",
    "test": "jest",
    "start:dev": "cp ./src/__fixtures__/access.log.template /tmp/access.log && ts-node src/index.ts",
